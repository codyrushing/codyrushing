<canvas class="animated-bg"></canvas>
<style>
  .animated-bg {
    z-index: 0;
    position: absolute;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
  }
</style>
<script>
import createBackgroundViz from './background-viz';
const canvasElements = document.querySelectorAll('canvas.animated-bg');
for(const canvasElement of canvasElements){
  createBackgroundViz(canvasElement as HTMLCanvasElement);
}
</script>
